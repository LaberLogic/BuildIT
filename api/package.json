{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch ./ --ext ts,json --ignore node_modules --ignore dist --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
    "start:prod": "node dist/index.js",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "migrate:dev": "prisma migrate dev --name",
    "migrate:reset": "prisma migrate reset --force",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "migrate:data": "node prisma/seed.js",
    "schema:change": "pnpm run migrate:reset && pnpm run prisma:generate && pnpm run migrate:data"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.8.2",
    "@types/jsonwebtoken": "^9.0.9",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "envalid": "^8.0.0",
    "express": "^5.1.0",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "neverthrow": "^8.2.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "prisma": "^6.8.2",
    "ts-jest": "^29.3.3"
  }
}
